package week1;

public class main003 {

	public static void main(String[] args) {

		for (int i = 0; i < 5; i++) {
			printSpace(5 - i);
			printStars(i + 1);
			printSpace(5 - i);
		}
	}

	public static void printSpace(int num) {
		for (int i = 0; i < num; i++) {
			System.out.print(" ");
		}
	}

	public static void printStars(int num) {
		for (int i = 0; i < num; i++) {
			System.out.print("*");
		}
	}

}

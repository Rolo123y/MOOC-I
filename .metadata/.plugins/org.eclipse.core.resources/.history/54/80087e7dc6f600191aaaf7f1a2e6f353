package week1;

public class main003 {

	public static void main(String[] args) {
		int num = 0;
		for (int i = 0; i < 5; i++) {
			printSpace(5 - i + 1);
			printStars(num + 1);
			printStars(num + 1);
			num++;
			printSpace(5 - i + 1);
			System.out.println();
		}
	}

	public static void printSpace(int num) {
		for (int i = 0; i < num; i++) {
			System.out.print(" ");
		}
	}

	public static void printStars(int num) {
		for (int i = 0; i < num; i++) {
			System.out.print("*");
		}
	}

}
